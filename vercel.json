{
    "version": 2,
    "builds": [
        {
            "src": "web/**",
            "use": "@vercel/static"
        },
        {
            "src": "api/*.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "/web/index.html"
        },
        {
            "src": "/web/(.*)",
            "dest": "/web/$1"
        },
        {
            "src": "/feedback(.*)",
            "dest": "api/feedback.py"
        },
        {
            "src": "/api/subscribe(.*)",
            "dest": "api/subscribe.py"
        },
        {
            "src": "/subscribe(.*)",
            "dest": "api/subscribe.py"
        },
        {
            "src": "/unsubscribe(.*)",
            "dest": "api/unsubscribe.py"
        }
    ]
}
